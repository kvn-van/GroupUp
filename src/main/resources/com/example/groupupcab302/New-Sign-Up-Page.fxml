<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vbox" prefHeight="600.0" prefWidth="480.0" styleClass="blue-pane" stylesheets="@styling/styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label prefHeight="35.0" prefWidth="130.0" text="GroupUp!" textFill="WHITE">
         <font>
            <Font name="Eras Demi ITC" size="26.0" />
         </font>
         <VBox.margin>
            <Insets left="20.0" top="20.0" />
         </VBox.margin>
      </Label>
      <Label prefHeight="35.0" prefWidth="75.0" text="Sign Up" textFill="#2196f3">
         <VBox.margin>
            <Insets left="20.0" top="20.0" />
         </VBox.margin>
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <HBox id="tf_box" alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="190.0">
               <children>
                  <TextField promptText="First Name" styleClass="tf_box" stylesheets="@styling/styles.css" HBox.hgrow="ALWAYS" />
               </children>
               <HBox.margin>
                  <Insets right="58.0" />
               </HBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="190.0">
               <children>
                  <TextField promptText="Last Name" styleClass="tf_box" stylesheets="@styling/styles.css" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="15.0" />
         </VBox.margin>
      </HBox>
      <HBox id="tf_box" alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="480.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="190.0">
               <children>
                  <TextField promptText="Phone Number" styleClass="tf_box" stylesheets="@styling/styles.css" HBox.hgrow="ALWAYS" />
               </children>
               <HBox.margin>
                  <Insets right="58.0" />
               </HBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="190.0">
               <children>
                  <TextField promptText="Age" styleClass="tf_box" stylesheets="@styling/styles.css" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="15.0" />
         </VBox.margin>
      </HBox>
      <HBox id="tf_box" alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
         <children>
            <TextField promptText="Username" styleClass="tf_box" stylesheets="@styling/styles.css" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets left="15.0" right="25.0" top="15.0" />
         </VBox.margin>
      </HBox>
      <HBox id="tf_box" alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
         <children>
            <TextField promptText="Email" styleClass="tf_box" stylesheets="@styling/styles.css" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets left="15.0" right="25.0" top="15.0" />
         </VBox.margin>
      </HBox>
      <HBox id="tf_box" alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
         <children>
            <TextField promptText="Password" styleClass="tf_box" stylesheets="@styling/styles.css" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin></TextField>
         </children>
         <VBox.margin>
            <Insets left="15.0" right="25.0" top="15.0" />
         </VBox.margin>
      </HBox>
      <HBox id="tf_box" alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
         <children>
            <TextField promptText="Password Confirmation" styleClass="tf_box" stylesheets="@styling/styles.css" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets left="15.0" right="25.0" top="15.0" />
         </VBox.margin>
      </HBox>
      <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="483.0" styleClass="blue-btn" text="Sign Up">
         <VBox.margin>
            <Insets bottom="25.0" left="15.0" right="15.0" top="20.0" />
         </VBox.margin>
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <HBox id="tf_box" alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
         <children>
            <TextArea prefHeight="200.0" prefWidth="200.0" promptText="Sign Up Status: Any errors will be displayed here" styleClass="ta" stylesheets="@styling/styles.css" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets left="15.0" right="15.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
